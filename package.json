{
  "name": "beagle",
  "version": "0.1.0",
  "description": "A CLI tool that sniffs out unused exports in TypeScript and Javascript.",
  "main": "./lib/index.js",
  "bin": {
    "beagle": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && beagle",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfritton/beagle.git"
  },
  "author": "Sam Fritton",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sfritton/beagle/issues"
  },
  "homepage": "https://github.com/sfritton/beagle#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^7.2.0",
    "figlet": "^1.5.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/clear": "^0.1.1",
    "@types/figlet": "^1.5.0",
    "@types/node": "^14.14.40",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}